    @media $media-tablet-vertical
      width 123px
      height 21px


  &__modal-button
    color $link
    $regularFont()
    text-decoration underline
    font-weight 800

    @media $media-tablet-vertical
      display none

    &_sidebar
      @media $media-tablet-vertical
        display inline-block
        line-height 22px
        margin-top 24px
        padding 0

  &__right
    display flex
    gap 80px

  &__socials
    display flex
    gap 34px

    @media $media-tablet-vertical
      display none

  &__phone
    position relative
    display flex
    align-items center
    gap 8px

    & span
      @media $media-tablet-vertical
        display none

  &__bottom
    border-top 1px solid $gray-border
    padding 24px 0

    @media $media-tablet-vertical
      display none

  &__menu
    position relative
    left 50%
    transform translateX(-50%)
    display inline-flex
    align-items center

    &-item
      display flex
      flex-direction column
      align-items center
      $navigationFont()
      font-weight 700

      & + &
        margin-left 56px

        @media $media-tablet
          margin-left 3.64vw

      &_logo
        position relative
        padding-top 13px

        &::before
          content ''
          display block
          position absolute
          top 0
          left 50%
          transform translateX(-50%)
          width 74px
          height 13px
          background-position center
          background-size cover
          background-repeat no-repeat

