@import "vars.styl"

.header

  &__burger
    display none
    width 24px
    height 24px
    cursor pointer

    @media $media-tablet-vertical
      display block

    &-line
      position relative
      display block
      top 50%
      left 50%
      transform translate(-50%, 50%)
      width 18px
      height 2px
      background-color black

      &::before , &::after
        content ''
        position absolute
        left 0
        width 100%
        height 100%
        top -6px
        background-color black

      &::after
        top 6px


  &__top
    padding 32px 0
    display flex
    justify-content space-between
    align-items center

    @media $media-mobile
      padding 12px 0 24px

  &__left
    display flex
    align-items center
    gap 88px


